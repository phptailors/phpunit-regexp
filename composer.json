{
    "name": "phptailors/phpunit-regexp",
    "description": "PCRE assertions for PHPUnit-based tests",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/phptailors/phpunit-extensions/issues",
        "source": "https://github.com/phptailors/phpunit-extensions"
    },
    "authors": [
        {
            "name": "Pawe≈Ç Tomulik",
            "email": "pawel@tomulik.pl"
        }
    ],
    "conflict": {
        "nikic/php-parser": "< 4.13.2"
    },
    "require": {
        "php": ">=8.2",
        "phptailors/phpunit-common": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "Tailors\\PHPUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tailors\\PHPUnit\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
